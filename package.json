{
  "name": "@bjca/browser",
  "version": "0.0.1",
  "description": "CA's signing, verifying under browser via bjca",
  "main": "./dist/bjca.js",
  "scripts": {
    "build": "npm run clean && npm run lint && npm run build:ts && npm run jslint",
    "build:ts": "tsc -p ./ --outDir dist/",
    "clean": "rm -rf dist/*",
    "cov": "nyc mocha",
    "cov:coveralls": "nyc mocha && nyc report --reporter=text-lcov | coveralls",
    "jslint": "eslint --fix dist/**/*.js",
    "lint": "tslint -p tslint.json -e **/*.d.ts -t stylish src/**/*.ts test/**/*.ts",
    "prepublishOnly": "npm run build",
    "test": "npm run test:travis",
    "test:appveyor": "mocha --config test/mocha.appveyor.opts",
    "test:travis": "mocha --config test/mocha.travis.opts"
  },
  "keywords": [],
  "nyc": {
    "exclude": [
      "**/*.js",
      "**/*.test.ts",
      "**/*.d.ts",
      "test",
      "dist",
      "src/bin"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "all": true
  },
  "module": "./dist/bjca.js",
  "types": "./dist/bjca.d.ts",
  "author": "waiting",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/waitingsong/bjca.js.git"
  },
  "bugs": {
    "url": "https://github.com/waitingsong/bjca.js/issues"
  },
  "homepage": "https://github.com/waitingsong/bjca.js#readme",
  "engines": {},
  "devDependencies": {
    "@types/mocha": "^2.2.48",
    "@types/power-assert": "^1.4.29",
    "@types/rewire": "^2.5.28",
    "@types/rimraf": "^2.0.2",
    "coveralls": "^3.0.0",
    "eslint": "^4.17.0",
    "intelli-espower-loader": "^1.0.1",
    "istanbul": "^0.4.5",
    "mocha": "^5.0.0",
    "mocha-lcov-reporter": "^1.3.0",
    "mz-modules": "^2.0.0",
    "nyc": "^11.4.1",
    "power-assert": "^1.4.4",
    "rewire": "^3.0.2",
    "rimraf": "^2.6.2",
    "source-map-support": "^0.5.3",
    "tslint": "^5.9.1",
    "tslint-eslint-rules": "^4.1.1",
    "typescript": "^2.7.2"
  }
}
